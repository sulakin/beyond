  <header class="header">
    {{#if @site.logo}}
      <a href="{{@site.url}}" title="{{t "Home"}}" class="logo">
        <img src="{{@site.logo}}" alt="{{t "Logo"}}" />
      </a>
    {{else if @site.title}}
      <a href="{{@site.url}}" title="{{t "Home"}}" class="logo">
        {{@site.title}}
      </a>
    {{/if}}

    {{navigation}}

    <ul>
      {{#if @site.twitter}}
        <li>
          <a href="{{twitter_url}}" 
            title="Twitter" 
            title="{{@site.twitter}}" 
            target="_blank">
